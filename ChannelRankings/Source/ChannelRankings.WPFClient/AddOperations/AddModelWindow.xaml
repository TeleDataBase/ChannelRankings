        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChannelRankings.WPFClient"
        mc:Ignorable="d"
        Title="AddModelWindow" Height="300" Width="300">
    <Grid>
        <Button x:Name="AddOwner" Content="Add new owner" HorizontalAlignment="Left" Margin="67,66,0,0" VerticalAlignment="Top" Width="150" Height="29" FontSize="14" FontWeight="Bold" Click="AddOwner_Click"/>
        <Button x:Name="AddSponsor" Content="Add new sponsor" HorizontalAlignment="Left" Margin="67,160,0,0" VerticalAlignment="Top" Width="150" Height="29" FontSize="14" FontWeight="Bold" Click="AddSponsor_Click"/>

    </Grid>
</Window>
