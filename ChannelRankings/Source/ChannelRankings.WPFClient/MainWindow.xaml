<Window x:Class="ChannelRankings.WPFClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChannelRankings.WPFClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Grid>
        <Button x:Name="importXmlButton" Content="Import XML" HorizontalAlignment="Left" Margin="68,33,0,0" VerticalAlignment="Top" Width="151" Height="29" FontSize="16" FontWeight="Bold" Click="importXmlButton_Click"/>
        <Button x:Name="GeneratePdfReport" Content="Generate Pdf Report" HorizontalAlignment="Left" Margin="281,33,0,0" VerticalAlignment="Top" Width="180" Height="29" FontSize="16" FontWeight="Bold" Click="GeneratePdfReport_Click"/>
        <Button x:Name="CreateModelButton" Content="Create Model" HorizontalAlignment="Left" Margin="68,118,0,0" VerticalAlignment="Top" Width="151" Height="29" FontSize="16" FontWeight="Bold" Click="CreateModelButton_Click"/>

    </Grid>
</Window>
